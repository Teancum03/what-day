    "react-jdenticon": "^1.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.2",
    "sass": "^1.50.1",
    "supertest": "^6.2.2",
    "tailwindcss": "^3.3.5",
    "tsx": "^3.12.7",
    "typescript": "^5.2.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead"
}
