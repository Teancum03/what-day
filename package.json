{
  "name": "what-day-is-it",
  "type": "module",
  "version": "0.1.0",
  "description": "A handy app to remind bootcampers what day it is and give handy tips!",
  "repository": "https://https://github.com/manaia-2023/what-day-is-it",
  "main": "server/index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "test": "vitest",
    "test-coverage": "vitest -- --coverage",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [
    "fullstack"
  ],
  "author": "Dev Academy Aotearoa",
  "license": "ISC",
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.456.0",
    "@aws-sdk/lib-storage": "^3.456.0",
    "@aws-sdk/s3-request-presigner": "^3.456.0",
    "@devacademy/eslint-config": "^1.9.1",
    "@radix-ui/react-form": "^0.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.17.2",
    "knex": "^2.5.1",
    "langchain": "^0.0.192",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.19.0",
    "pexels": "^1.4.0",
    "sqlite3": "^5.1.6",
    "superagent": "7.1.1"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.9.1",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@joaomoreno/unique-names-generator": "^5.1.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/themes": "^2.0.1",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/superagent": "^4.1.18",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.19.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
